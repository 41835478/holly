{
    "name": "elfsundae/holly",
    "description": "An app framework built with Laravel.",
    "keywords": [
        "holly",
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "type": "project",
    "homepage": "https://github.com/ElfSundae/holly",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "rap2hpoutre/laravel-log-viewer": "~0.7.1",
        "backup-manager/laravel": "^1.1",
        "guzzlehttp/guzzle": "~6.2",
        "jenssegers/agent": "~2.3.3",
        "iatstuti/laravel-nullable-fields": "~1.3",
        "ramsey/uuid": "^3.5",
        "vinkla/hashids": "^3.0",
        "jenssegers/optimus": "^0.2.2",
        "intervention/image": "^2.3",
        "mews/captcha": "^2.1",
        "simplesoftwareio/simple-qrcode": "^1.4",
        "yajra/laravel-datatables-oracle": "^6.22",
        "aporat/store-receipt-validator": "^1.4",
        "laravel-notification-channels/bearychat": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~0.9.4",
        "phpunit/phpunit": "~5.4",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "doctrine/dbal": "~2.5",
        "barryvdh/laravel-debugbar": "~2.3",
        "barryvdh/laravel-ide-helper": "~2.2",
        "caouecs/laravel-lang": "~3.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/Support/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate",
            "php artisan key:api-token"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate-helpers",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
